<% provide :title, "Edit Category" %>
<h1 class="page-header">Edit Category</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @category, url: admin_category_path(@category) do |f| %>
      <%= render "form_fields", f: f, button_name: "Update" %>
    <% end %>
  </div>
</div>